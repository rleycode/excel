# MySQL Database Configuration для Ubuntu OpenCart сервера
# Замените на реальные данные вашего сервера
DB_HOST=your-server-ip-or-domain.com
DB_PORT=3306
DB_USER=excel_parser_user
DB_PASSWORD=your_secure_password
DB_NAME=excel_parser_db

# SSL настройки (рекомендуется для удаленного подключения)
DB_SSL_DISABLED=true
DB_SSL_CA_PATH=
DB_SSL_CERT_PATH=
DB_SSL_KEY_PATH=

# Настройки подключения
DB_CONNECTION_TIMEOUT=30
DB_AUTOCOMMIT=true
